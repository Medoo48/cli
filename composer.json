{
	"name": "leafs/cli",
	"description": "A simple command line tool for installing and interacting with your leaf apps",
	"homepage": "https://cli.leafphp.dev",
	"version": "v2.9.2-beta",
	"keywords": [
		"leaf",
		"php",
		"installer",
		"deploy",
		"server",
		"database"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Michael Darko",
			"email": "mickdd22@gmail.com",
			"homepage": "https://mychi.netlify.app",
			"role": "Developer"
		}
	],
	"require": {
		"ext-json": "*",
		"leafs/fs": "^1.0",
		"psy/psysh": "*",
		"symfony/console": "^5.0|^6.0",
		"symfony/process": "^5.0|^6.0"
	},
	"bin": [
		"bin/leaf"
	],
	"autoload": {
		"psr-4": {
			"Leaf\\Console\\": "src/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
