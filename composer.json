{
	"name": "leafs/cli",
	"description": "A simple command line tool for installing and interacting with your leaf apps",
	"homepage": "https://cli.leafphp.dev",
	"version": "v2.0.5",
	"keywords": [
		"leaf",
		"php",
		"installer"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Michael Darko",
			"email": "mickdd22@gmail.com",
			"homepage": "https://mychi.netlify.app",
			"role": "Developer"
		}
	],
	"require": {
		"ext-json": "*",
		"leafs/fs": "^1.0",
		"psy/psysh": "^0.11.1",
		"symfony/console": "^6.0",
		"symfony/process": "^6.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.0"
	},
	"bin": [
		"bin/leaf"
	],
	"autoload": {
		"psr-4": {
			"Leaf\\Console\\": "src/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
